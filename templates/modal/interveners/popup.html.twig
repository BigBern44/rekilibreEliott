<div aria-hidden="true" aria-labelledby="modalFormPopupLabel" class="modal fade" id="modalFormPopup{{id}}" role="dialog" tabindex="-1">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            {{form_start(formListeIntervener)}}
            <div class="modal-header">
                <h5 class="modal-title" id="modalFormIntervenerLabel">Choisir une Date</h5>
                <button aria-label="Close" class="close" data-dismiss="modal" type="button">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>
            {{ form_row(formListeIntervener.FromDate) }}
            {{ form_row(formListeIntervener.ToDate) }}
            {{ form_row(formListeIntervener.Id, {'value' : id}) }}
            <div class="modal-footer">
                {{ form_widget(formListeIntervener.Valider,{'attr':{'class':'btn btn-success'}}) }}
                <button class="btn btn-danger" data-dismiss="modal" type="button">Fermer</button>
            </div>

            {{form_end(formListeIntervener)}}
        </div>
    </div>
</div>
